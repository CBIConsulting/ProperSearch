{
  "name": "react-propersearch",
  "version": "0.1.15",
  "description": "A react component that render a search field with a list of items, allows the user to filter that list and select the items. The component return the selected data when it get selected",
  "main": "lib/propersearch.js",
  "scripts": {
    "test": "webpack && karma start --single-run --no-auto-watch karma.conf.js",
    "watch": "karma start --auto-watch --no-single-run karma.conf.js",
    "start": "webpack-dev-server --hot --inline --config webpack.config.example.js",
    "build": "babel src/jsx --out-dir lib && webpack && cp -rvf src/css/* css/",
    "release": "webpack --config webpack.config.min.js",
    "build-example": "webpack --config webpack.config.example.dist.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CBIConsulting/ProperSearch.git"
  },
  "keywords": [
    "react-component",
    "search",
    "filter",
    "ui"
  ],
  "author": "cbi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CBIConsulting/ProperSearch/issues"
  },
  "homepage": "https://github.com/CBIConsulting/ProperSearch#readme",
  "devDependencies": {
    "babel-core": "^6.7.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-es3-member-expression-literals": "^6.5.0",
    "babel-plugin-transform-es3-property-literals": "^6.5.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "compass-mixins": "^0.12.7",
    "core-js": "^2.2.1",
    "css-loader": "^0.23.1",
    "eslint": "^2.5.3",
    "eslint-plugin-react": "^4.2.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "imports-loader": "^0.6.5",
    "jasmine": "^2.4.1",
    "jasmine-core": "^2.4.1",
    "jquery": "^2.2.3",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.2.3",
    "karma-cli": "^0.1.2",
    "karma-coverage": "^0.5.5",
    "karma-jasmine": "^0.3.8",
    "karma-notification-reporter": "0.0.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "node-sass": "~3.4.2",
    "phantomjs-prebuilt": "^2.1.7",
    "react-addons-test-utils": "^0.14.7",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "es6-set": "^0.1.4",
    "immutable": "^3.7.6",
    "react": "^0.14.7",
    "react-dimensions": "^1.0.2",
    "react-dom": "^0.14.7",
    "react-addons-shallow-compare": "^0.14.7",
    "react-immutable-render-mixin": "^0.9.5",
    "react-virtualized": "^6.0.2",
    "underscore": "^1.8.3"
  }
}
